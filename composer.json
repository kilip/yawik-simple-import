{
    "type": "yawik-module",
    "name": "yawik/simple-import",
    "description": "This module provides various imports",
    "license": "MIT",
    "authors": [
        {
            "name": "Carsten Bleek",
            "email": "bleek@cross-solution.de"
        },
        {
            "name": "Mathias Gelhausen",
            "email": "gelhausen@cross-solution.de"
        },
        {
            "name": "Miroslav Fedeles",
            "email": "miroslav.fedeles@gmail.com"
        },
        {
            "name": "Anthonius Munthi",
            "email": "me@itstoni.com"
        }
    ],
    "keywords": [
        "yawik", "module", "zf3"
    ],
    "config": {
        "platform": {
            "ext-mongo": "1.6.16"
        },
        "process-timeout": 0
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "yawik/auth": ">=0.32",
        "yawik/jobs": ">=0.32",
        "yawik/settings": ">=0.32",
        "yawik/applications": ">=0.32",
        "yawik/cv": ">=0.32",
        "yawik/organizations": ">=0.32",
        "yawik/composer-plugin": ">=0.32",
        "yawik/core": ">=0.32",
        "yawik/geo": ">=0.32",
        "willdurand/geocoder": "^3.3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "yawik/behat": "^0.32@dev",
        "cross-solution/phpunit-utils": "2.0.x-dev"
    },
    "autoload": {
        "psr-4": { "SimpleImport\\": "src/"}
    },
    "autoload-dev": {
	"psr-4": {
            "SimpleImportTest\\": "test/SimpleImportTest/",
	    "CoreTestUtils\\": "vendor/yawik/core/test/CoreTestUtils/"
	},
        "psr-0": {
            "": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.32-dev"
        },
        "zf": {
            "module": "SimpleImport"
        }
    },
    "scripts": {
        "auto-scripts": [
            "npm install"
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "serve": "php -S localhost:8000 -t test/sandbox/public",
        "start-selenium": "./vendor/bin/selenium-server-standalone -p 4444 -Dwebdriver.chrome.driver=./vendor/bin/chromedriver",
        "test": "phpunit"
    }
}
