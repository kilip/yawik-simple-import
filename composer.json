{
    "type": "yawik-module",
    "name": "yawik/simple-import",
    "description": "This module provides various imports",
    "license": "MIT",
    "authors": [
        {
            "name": "Carsten Bleek",
            "email": "bleek@cross-solution.de"
        },
        {
            "name": "Mathias Gelhausen",
            "email": "gelhausen@cross-solution.de"
        },
        {
            "name": "Miroslav Fedeles",
            "email": "miroslav.fedeles@gmail.com"
        },
        {
            "name": "Anthonius Munthi",
            "email": "me@itstoni.com"
        }
    ],
    "keywords": [
        "yawik",
        "module",
        "zf3"
    ],
    "config": {
        "platform": {
            "ext-mongo": "1.6.16"
        },
        "process-timeout": 0,
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
        "geocoder-php/google-maps-provider": "^4.4",
        "guzzlehttp/guzzle": "^6.3",
        "php-http/cache-plugin": "^1.6",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message": "^1.7",
        "yawik/jobs": ">=0.32"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5.14 || ^7.5.13",
        "yawik/applications": ">=0.32",
        "yawik/auth": ">=0.32",
        "yawik/behat": "^0.32@dev",
        "yawik/composer-plugin": ">=0.32",
        "yawik/core": "dev-develop",
        "yawik/cv": ">=0.32",
        "yawik/geo": ">=0.32",
        "yawik/install": "^0.33.14",
        "yawik/organizations": ">=0.32",
        "yawik/settings": ">=0.32"
    },
    "autoload": {
        "psr-4": {
            "SimpleImport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleImportTest\\": "test/SimpleImportTest/",
            "CoreTestUtils\\": "vendor/yawik/core/test/CoreTestUtils/"
        },
        "psr-0": {
            "": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.32-dev"
        },
        "zf": {
            "module": "SimpleImport"
        }
    },
    "scripts": {
        "auto-scripts": [
            "npm install"
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "serve": "php -S localhost:8000 -t test/sandbox/public",
        "start-selenium": "./vendor/bin/selenium-server-standalone -p 4444 -Dwebdriver.chrome.driver=./vendor/bin/chromedriver",
        "test": "phpunit",
        "db.init": "mongorestore --drop"
    }
}
